{
  "expo": {
    "name": "FindImagic",
    "scheme": "FindImagic",
    "slug": "FindImagic",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/FIMreal.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splashreal.png",
      "resizeMode": "contain",
      "backgroundColor": "#fd7014"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.anonymous.FindImagic"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/FIMreal.png",
        "backgroundColor": "#fd7014"
      },
      "package": "com.anonymous.FindImagic"
    },
    "web": {
      "favicon": "./assets/favicon.png",
      "bundler": "metro"
    },
    "plugins": [
      "expo-router",
      "expo-localization"
    ],
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "c74d327c-d1de-4663-8c7a-6cbc528a56fc"
      }
    }
  }
}
